  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "heroku-postbuild": "ng build web-app --prod && ng build server --prod",
    "test": "ng test",
    "lint": "./node_modules/.bin/nx lint && ng lint",
    "e2e": "ng e2e",
    "firebase:serve": "concurrently --kill-others \"yarn run build functions --watch\" \"firebase serve --only functions\"",
